<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProyectoFinal" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="android:statusBarColor">@color/primaryColor</item>
        <item name="android:textColorLink">@color/primaryColor</item>
    </style>

    <style name="MyDatePickerStyle" parent="@android:style/Widget.Material.DatePicker">
        <item name="colorAccent">@color/primaryColor</item>
    </style>

    <style name="Splash_Theme" parent="Theme.ProyectoFinal">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>
    <style name="Theme.ProyectoFinal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.ProyectoFinal.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.ProyectoFinal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>